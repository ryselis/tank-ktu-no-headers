        <Image x:ConnectionId='9' x:Name="fireImageButton" HorizontalAlignment="Left" Height="22" Margin="130,717,0,0" VerticalAlignment="Top" Width="78" Source="Assets/CFire.png"                                                                                                                                                                                                />
