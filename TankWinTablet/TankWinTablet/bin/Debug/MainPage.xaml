        <ListView HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100"/>
        <Image x:ConnectionId='1' x:Name="ForwardButtonImage" HorizontalAlignment="Left" Height="24" Margin="87,631,0,0" VerticalAlignment="Top" Width="140"                                              Source="Assets/Forward.png"                                                                                                                                                                    />
